   <!--End sidebar-wrapper-->

<!--Start topbar header-->
<header class="topbar-nav">
  <nav class="navbar navbar-expand fixed-top">
   <ul class="navbar-nav mr-auto align-items-center">
     <li class="nav-item">
       <a class="nav-link toggle-menu" href="javascript:void();">
        <i class="icon-menu menu-icon"></i>
      </a>
     </li>
     <div class="nav-item">
        <form class="search-bar"action="/admin/product-Search" method ="post">
         <div class="d-flex">
           <input type="text" name="search" class="form-control" placeholder="Search Here.....">
          <button class="btn" type="submit"><i class="icon-magnifier"></i></button>
        </div>
        </form>
      </div>
   </ul>
     <li class="nav-item"style="list-style: none;">
       <a class="nav-link dropdown-toggle dropdown-toggle-nocaret" data-toggle="dropdown" href="">
         <span class="user-profile"><img src="data:image/jpeg;base64," class="img-circle" alt="user avatar"></span>
       </a>
       <ul class="dropdown-menu dropdown-menu-right" style="background: linear-gradient(to right, #ffffff48, #ffffff4d);">
        <li class="dropdown-item user-details" style="background-color: transparent;">
             <a class="text-dark" href="/admin/productCreate">Add Product</a>
         </li>
       </ul>
     </li>
   </ul>
 </nav>
 </header>
<!--End topbar header-->

<div class="clearfix"></div>
	
  <div class="content-wrapper">
    <div class="container-fluid">
     
      <div class="row mt-5">
        <div class="col-lg-12">
          <div class="card">
            <div class="card-body">
              <h5 class="card-title">Products</h5>
			  <div class="table-responsive">
               <table class="table">
                  <thead>
                    <tr>
                      <th scope="col">Name</th>
                      <th scope="col">Category</th>
                      <th scope="col">Offer</th>
                      <th scope="col">Price</th>
                      <th scope="col">Real Price</th>
                      <th scope="col">Quantity</th>
                      <th scope="col">Images</th>
                      <th scope="col">Availability</th>
                      <th scope="col">Edit</th>
                    </tr>
                  </thead>
                  <tbody>
                    <% product.forEach(element => { %>
                        <tr id="coupon-">
                            <td id="pro-body"><%= element.name %></td>
                            <td id="pro-body"><%= element.category %></td>
                            <td id="pro-body"><%= element.productOffer %> %</td>
                            <td id="pro-body"><%= element.price %></td>
                            <td id="pro-body"><%= element.orginalprice %></td>
                            <td id="pro-body"><%= element.quantity %></td>
                            <td id="pro-body">
                                <img style="width: 50px; height: 55px" src="/productImages/<%= element.image[0] %>" alt="<%= element.name %>" />
                            </td>
                            <td id="pro-body">
                                <% if (element.availability === true) { %>
                                    <form action="/admin/productUnlist/<%= element._id %>" method="POST">
                                        <button class="btn btn-danger btn-sm" type="button" onclick="list(this.form)">Unlist</button>
                                    </form>
                                <% } else if (element.availability === false) { %>
                                    <form action="/admin/productList/<%= element._id %>" method="POST">
                                        <button class="btn btn-success btn-sm" type="button" onclick="unlist(this.form)">List</button>
                                    </form>
                                <% } %>
                            </td>
                            <td id="pro-body">
                                <a href="/admin/product-edit/<%= element._id %>" class="edit"
                                  title="Edit"
                                  data-toggle="tooltip">
                                  <span class="material-symbols-outlined">
                                    edit
                                    </span>
                                </a>
                            </td>
                        </tr>
                    <% }) %>
                </tbody>
            </table>
            </div>
            </div>
          </div>
        </div>
		  <div class="overlay toggle-menu"></div>
    </div>
    </div>
    <a href="javaScript:void();" class="back-to-top"><i class="fa fa-angle-double-up"></i> </a>
 
   
<script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
  



<script>

  function list(form) {
    swal({
      title: "Are you sure?",
      text: "You want to list the product!",
  
      buttons: true,
      dangerMode: true,
    }).then((willDelete) => {
      if (willDelete) {
        form.submit();
      }
    });
  }

  function unlist(form) {
    
    swal({
      title: "Are you sure?",
      text: "You want to unlist the product!",
      
      buttons: true,
      dangerMode: true,
    }).then((willDelete) => {
      if (willDelete) {
        form.submit();
      }
    });
  }
</script>

<script>
  $(document).ready(function () {
    $('[data-toggle="tooltip"]').tooltip();
  });
</script>


 <!-- Bootstrap core JavaScript-->
 <script src="/assets/js/jquery.min.js"></script>
 <script src="/assets/js/popper.min.js"></script>
 <script src="/assets/js/bootstrap.min.js"></script>
 
 <!-- simplebar js -->
 <script src="/assets/plugins/simplebar/js/simplebar.js"></script>
 <!-- sidebar-menu js -->
 <script src="/assets/js/sidebar-menu.js"></script>
 
 <!-- Custom scripts -->
 <script src="/assets/js/app-script.js"></script>

 
 
</body>
</html>
