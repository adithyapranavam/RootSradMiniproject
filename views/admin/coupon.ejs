<div class="clearfix"></div>
	
  <div class="content-wrapper">
    <div class="container-fluid">
     
      <div class="row mt-6">
        <div class="col-lg-12">
          <div class="card">
            
            
            </div>
          </div>
        </div>
      <div class="row">
        <div class="col-lg-12">
          <div class="card">
            <div class="card-body">
              <h5 class="card-title">Coupon List</h5>
			  <div class="table-responsive">
               <table class="table table-striped">
                  <thead>
                    <tr>
                      <th> Name  </th>
                  <th> Value</th>
                  <th> Exp. Date </th>
                  <th> Min Value </th>
                  <th> Max Value </th>
                  <th> Edit </th>
                  <th> Delete </th>
                    </tr>
                  </thead>
                  
                  <tbody id="body">
                    <% if(locals.couponData){ %>
                        <% couponData.forEach((data)=>{ %>
                        <tr >
                         
                          <td>
                            <%= data.couponName %>
                          </td>
                          
                          <td>
                            <%= data.couponValue %> %
                          </td>

                          <td >
                            <%= data.expiryDate %>
                          </td>

                          <td>
                              <%= data.minValue %>
                          </td>
                          <td>
                            <%= data.maxValue %>
                          </td>
                          <td id="pro-body">
                            <a href="/admin/editCoupon?id=<%= data._id %>" class="edit"
                              title="Edit"
                              data-toggle="tooltip">
                              <span class="material-symbols-outlined">
                                edit
                                </span>
                            </a>
                        </td>
                          <td>
                            <a href="/admin/deleteCoupon?id=<%= data._id  %>" class="Delete" style="color: red;" >Delete</a>
                        </td>
                        </tr>
                        <% }) %>
                          <% } %>
                  </tbody>
                </table>
              </div>
	  

